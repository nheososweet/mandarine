# Copy từ .env.example và điền thông tin thực
PROJECT_NAME="Mandarine API"
DEBUG=True
API_V1_PREFIX="/api/v1"

# PostgreSQL - Điền thông tin database của bạn
POSTGRES_USER="postgres"
POSTGRES_PASSWORD=password
POSTGRES_HOST="localhost"
POSTGRES_PORT="5432"
POSTGRES_DB="mandarine"

# Database URL sẽ tự động build hoặc ghi trực tiếp:
DATABASE_URL="postgresql+asyncpg://postgres:password@localhost:5432/mandarine"
# Security
SECRET_KEY="change-this-to-random-string-in-production"

DB_ECHO_SQL=True  

GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

LLM_BASE_URL="https://api.example-llm.com/v1"